# ================================
# CityLens Project .gitignore
# ================================

# ---- Environment & Secrets ----
.env
.env.local
.env.*.local
*.pem
**/credentials*.json
**/secrets*.json

# ---- Python (Backend) ----
backend/venv/
backend/__pycache__/
backend/**/__pycache__/
backend/**/*.pyc
backend/**/*.pyo
backend/**/*.pyd
*.egg-info/
.eggs/
dist/
build/
*.so

# ---- Node.js (Frontend) ----
frontend/node_modules/
frontend/.next/
frontend/out/
frontend/.pnp
frontend/.pnp.*
frontend/.yarn/*
!frontend/.yarn/patches
!frontend/.yarn/plugins
!frontend/.yarn/releases
!frontend/.yarn/versions

# ---- IDE & Editors ----
.vscode/
.idea/
*.swp
*.swo
*~

# ---- OS Files ----
.DS_Store
Thumbs.db

# ---- Logs ----
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.pnpm-debug.log*

# ---- Testing & Coverage ----
coverage/
.coverage
htmlcov/
.pytest_cache/

# ---- Build Outputs ----
*.tsbuildinfo

# ---- Vercel ----
.vercel

# ---- Model Files (large) ----
*.pt
*.pth
*.onnx
*.safetensors

# ---- Temporary Files ----
tmp/
temp/
*.tmp
